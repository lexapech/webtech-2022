<dialog class="w3-card-4 w3-animate-top w3-round w3-modal-content" id="giveDialog" style="border: 0;padding: 0">
        <div class="w3-container padding-l-r-0">
                <span onclick="hideGiveModal()"
                      class="w3-button w3-display-topright w3-round">&times;</span>
                <div class="w3-container w3-center w3-teal margin-l-r-auto">
                    <p>Выдать книгу</p>
                </div>
                <div class="w3-container padding-t-b-20">
                    <label>ФИО читателя</label>
                    <input class="w3-input" type="text" id="readerName" >


                </div>
                <div class="w3-container padding-t-b-20">
                    <label>Дата возврата</label>
                    <input class="w3-input" type="date" id="returnDate">
                </div>
                <div class="w3-container w3-center margin-l-r-auto padding-t-b-20">
                    <input class="w3-button w3-border" type="button" value="Готово" onclick="give()" >
                </div>
        </div>
</dialog>